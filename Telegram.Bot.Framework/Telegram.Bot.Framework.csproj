<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyVersion>0.0.0.1</AssemblyVersion>
    <FileVersion>0.0.0.1</FileVersion>
    <NeutralLanguage />
    <Company>Sokushu</Company>
    <PackageProjectUrl>https://github.com/sokushu/Telegram.Bot.Net/</PackageProjectUrl>
    <Authors>Sokushu</Authors>
    <Version>0.0.0.1</Version>
    <Copyright>https://github.com/sokushu/Telegram.Bot.Net/</Copyright>
    <Description>一个使用简单的Telegram Bot框架</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.1.28" />
    <PackageReference Include="Telegram.Bot" Version="18.0.0" />
    <PackageReference Include="Telegram.Bot.Extensions.Polling" Version="1.0.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="InternalFramework\Filters\" />
    <Folder Include="InternalFramework\Authentications\" />
  </ItemGroup>

</Project>
