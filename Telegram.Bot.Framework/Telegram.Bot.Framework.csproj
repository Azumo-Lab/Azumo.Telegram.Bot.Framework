<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <AssemblyVersion>0.0.0.1</AssemblyVersion>
    <FileVersion>0.0.0.1</FileVersion>
    <NeutralLanguage />
    <Company>Azumo-Lab</Company>
    <PackageProjectUrl>https://github.com/Azumo-Lab/Telegram.Bot.Framework</PackageProjectUrl>
    <Authors>Azumo-Lab</Authors>
    <Version>0.0.0.1</Version>
    <Copyright>https://github.com/Azumo-Lab/Telegram.Bot.Framework</Copyright>
    <Description>一个使用简单的Telegram Bot框架</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Title>Telegram.Bot.Framework</Title>
    <RepositoryUrl>https://github.com/Azumo-Lab/Telegram.Bot.Framework</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.1" />
    <PackageReference Include="Telegram.Bot" Version="18.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Telegram.Bot.Framework.Abstract\Telegram.Bot.Framework.Abstract.csproj" />
    <ProjectReference Include="..\Telegram.Bot.Framework.Authentication\Telegram.Bot.Framework.Authentication.csproj" />
    <ProjectReference Include="..\Telegram.Bot.Framework.Controller\Telegram.Bot.Framework.Controller.csproj" />
    <ProjectReference Include="..\Telegram.Bot.Framework.Exceptions\Telegram.Bot.Framework.Exceptions.csproj" />
    <ProjectReference Include="..\Telegram.Bot.Framework.Helper\Telegram.Bot.Framework.Helper.csproj" />
    <ProjectReference Include="..\Telegram.Bot.Framework.Language\Telegram.Bot.Framework.Language.csproj" />
    <ProjectReference Include="..\Telegram.Bot.Framework.Logger\Telegram.Bot.Framework.Logger.csproj" />
  </ItemGroup>

</Project>
